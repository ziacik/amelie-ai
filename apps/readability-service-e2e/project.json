{
	"name": "readability-service-e2e",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"implicitDependencies": ["readability-service"],
	"targets": {
		"e2e": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
			"options": {
				"jestConfig": "apps/readability-service-e2e/jest.config.ts",
				"passWithNoTests": true
			}
		},
		"lint": {
			"executor": "@nx/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["apps/readability-service-e2e/**/*.{js,ts}"]
			}
		}
	}
}
